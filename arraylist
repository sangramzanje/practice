import java.util.ArrayList;

public class Main { 
  public static void main(String[] args) { 
    ArrayList<String> name = new ArrayList<String>();
    name.add("sangram");
    name.add("abhijit");
    name.add("prajwal");
    name.add("rathod");
    System.out.println(name);
  } 
}


import java.util.ArrayList;

public class Main { 
  public static void main(String[] args) { 
    ArrayList<String> cars = new ArrayList<String>();
    cars.add("Volvo");
    cars.add("BMW");
    cars.add("Ford");
    
    cars.add(0, "Mazda"); 

    System.out.println(cars);
  } 
}
